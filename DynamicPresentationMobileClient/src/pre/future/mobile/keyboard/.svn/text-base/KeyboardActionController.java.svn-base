package pre.future.mobile.keyboard;

import pre.future.connector.DeviceConnectionManager;
import android.util.Log;

public class KeyboardActionController {
	public void leftAction() { 
		Log.i("Check Log", "leftkey_button ");
		DeviceConnectionManager cc = DeviceConnectionManager.getInstance();
		cc.sendKeyboard("left");
	}
	
	public void rightAction() {
		Log.i("Check Log", "rightkey_button ");
		DeviceConnectionManager cc = DeviceConnectionManager.getInstance();
		cc.sendKeyboard("right");		
	}
	
	public void f5Actioin(){
		Log.i("Check Log", "rightkey_button ");
		DeviceConnectionManager cc = DeviceConnectionManager.getInstance();
		cc.sendKeyboard("f5");
	}
	
	public void escAction(){
		Log.i("Check Log", "rightkey_button ");
		DeviceConnectionManager cc = DeviceConnectionManager.getInstance();
		cc.sendKeyboard("esc");
	}
}